@model IEnumerable<dynamic>

<h3>All Notice</h3>
@if (Model != null)
{
    <table class="table table-striped table-responsive">
        <thead>
            <tr>
                <th>Notice Name</th>
                <th>Notice Date</th>
                <th>Notice Details</th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.NoticeName</td>
                    @try
                    {
                        <td>@item.NoticeDate.ToString("dd/MM/yyyy")</td>
                    }
                    catch
                    {

                    }
                    <td><a href="@Url.Action("NoticeDetails", "Notice", new { id = @item.ID })">Read more</a></td>
                </tr>
            }
        </tbody>
    </table>
}

