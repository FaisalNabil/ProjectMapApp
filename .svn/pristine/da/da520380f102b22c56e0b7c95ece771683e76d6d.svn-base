/* Mobile Layout: 320px. */
@media only screen and (max-width: 767px) { 
    
    .container{
        width: 100%
    }
    .big-responsive-menu{
        display: block
    }

    .mainmenu{
        display: none
    }
    .site-logo-img img{
        width: 75px
    }
    .site-logo-text > h1{
        font-size: 20px
    }
    .header-search > form{
        top: 45px
    }
    .mainmenu-area{
        min-height: 50px
    }
    .owl-dots{
        top: 80%
    }
    .single-promo{
        margin-right: 0
    }
    .page-content-left{
        border: medium none
    }
    .pad-right{
        padding-right: 15px
    }
    .page-content-area .col-md-4{
        padding-left: 15px
    }
    .single-footer-widget{
        margin: 30px 0
    }
    .copyright{
        text-align: center
    }
    .single-gallery-img{
        float: none;
        overflow: hidden
    }
    .gallery-img-itm{
        margin-right: 20px
    }
    .header-top-right-overlay::before {
        display:none
    }
    .header-top-right-overlay{
        display:none
    }
    .header-top-left{
        text-align:center
    }
    .single-top-right{
        display:none
    }
    .header-right-option{
        display:none
    }
    .site-logo{
        text-align:center
    }
    .big-main-caption h2{
        font-size:30px
    }
    .big-main-slide{
        height:320px
    }
    .single-notice-widget{
        margin-bottom:30px
    }
    .single-footer{
        margin:30px 0
    }

}
