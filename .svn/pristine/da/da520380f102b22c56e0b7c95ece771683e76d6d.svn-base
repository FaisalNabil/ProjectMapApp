/* Medium Layout: 1280px. */
@media only screen and (min-width: 992px) and (max-width: 1200px) { 

    .container{
        width: 100%
    }
    .site-responsive-menu{}
    .mainmenu{}
    .site-logo-img img{}
    .site-logo-text > h1{}
    .header-search > form{}
    .mainmenu-area{}
    .owl-dots{}
    .single-promo{}
    .page-content-left{}
    .pad-right{}
    .page-content-area .col-md-4{}
    .single-footer-widget{}
    .copyright{}
    .single-gallery-img{}
    
}
/* Tablet Layout: 768px. */
@media only screen and (min-width: 768px) and (max-width: 991px) { 

    .container{
        width: 100%
    }
    .site-responsive-menu{}
    .mainmenu{}
    .site-logo-img img{}
