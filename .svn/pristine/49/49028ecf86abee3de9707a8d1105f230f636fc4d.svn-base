/*================================================
02 - DEFAULT CSS
==================================================*/

body {
    font-family: "Open Sans", sans-serif;
    font-weight: 400;
    font-size: 15px;
    line-height: 26px;
    background: #DBC2A3;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

html,
body {
    height: 100%;
}

img {
    max-width: 100%;
    height: auto
}

p {
    margin: 0
}

.no-pad-left {
    padding-left: 0
}

.no-pad-right {
    padding-right: 0
}

.no-pad-all {
    padding: 0
}

h1,
h2,
h3,
h4,
h5,
h6 {
    margin: 0;
    padding: 0;
}

a {
    -webkit-transition: all 0.4s ease 0s;
    transition: all 0.4s ease 0s;
    color: #6b739c
}

    a:focus {
        text-decoration: none;
        outline: medium none;
        color: inherit
    }

    a:hover {
        color: inherit;
        text-decoration: none;
        color: inherit
    }

input:focus,
textarea:focus,
button:focus,
select:focus {
    outline: medium none
}

tbody,
tfoot,
thead,
tr,
th,
td {
    margin: 0;
    padding: 0;
    border: 0;
    font-size: 100%;
    font: inherit;
    vertical-align: baseline;
}

ul,
ol {
    margin: 0;
    padding: 0;
}

li {
    list-style-type: none;
}

.briffs-btn {
    display: inline-block;
    padding: 8px 20px;
    color: #fff !important;
    background: #fbba42 none repeat scroll 0 0;
    text-transform: capitalize;
    font-weight: 600;
    font-family: "Montserrat", sans-serif;
    letter-spacing: 1px;
}

    .briffs-btn:hover {
        background: #061333 none repeat scroll 0 0;
        color: #fff
    }

.site-heading {
    position: relative;
    padding-left: 30px;
    margin-bottom: 30px
}

    .site-heading:after {
        position: absolute;
        content: "";
        left: 0;
        height: 100%;
        width: 3px;
        background: #fbba42 none repeat scroll 0 0;
        top: 0;
    }

    .site-heading:before {
        position: absolute;
        content: "";
        left: 7px;
        height: 100%;
        width: 10px;
        background: #fbba42 none repeat scroll 0 0;
        top: 0;
    }

    .site-heading h4 {
        font-size: 18px;
        text-transform: capitalize;
        color: #fbba42;
        font-weight: 600;
        font-family: 'Montserrat', sans-serif;
    }

    .site-heading h3 {
        font-size: 36px;
        line-height: 40px;
        font-weight: 600;
        color: #061538;
        text-transform: capitalize;
        font-family: 'Montserrat', sans-serif;
    }

.page_bg {
    background: url(../img//page_bg.png) repeat scroll center center;
    padding-bottom: 20px;
    position: relative;
    z-index: 99;
}

    .page_bg::after {
        position: absolute;
        content: "";
        top: 0;
        left: 0;
        height: 100%;
        width: 100%;
        z-index: -9;
        opacity: 0.6;
        background: #e8bc87;
    }

.section_100 {
    padding: 100px 0;
}

.section_70 {
    padding: 70px 0;
}

.section_50 {
    padding: 50px 0;
}

.section_15 {
    padding: 15px 0
}

.pad-right {
    padding-right: 0
}

.section_t_100 {
