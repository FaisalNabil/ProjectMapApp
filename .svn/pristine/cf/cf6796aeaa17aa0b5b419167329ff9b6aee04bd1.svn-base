@model IEnumerable<dynamic>

<div class="title-sec inner-page-title">
    <h3>All Notice</h3>
</div>
@if (Model != null)
{
<div class="single-content-box">
    <div class="table-responsive">
        <table class="table table-striped table-responsive" style="width:100%">
            <thead>
                <tr>
                    <th>Notice Name</th>
                    <th>Notice Date</th>
                    <th>Notice Details</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td style="padding:10px">@item.NoticeName</td>
                        @try
                        {
                            <td>@item.NoticeDate.ToString("dd/MM/yyyy")</td>
                        }
                        catch
                        {

                        }
                        <td><a href="@Url.Action("NoticeDetails", "Notice", new { id = @item.NoticeID })">Read more</a></td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
}

