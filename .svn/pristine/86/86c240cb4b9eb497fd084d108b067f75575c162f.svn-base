/*===================
3-nav-area
===================*/

.nav-area{
    margin-top: 5px;
    position: relative;
}
.nav-area ul{
    background: #ffffff;
    border: 1px dotted #777777;
    box-shadow: 0px 3px 5px 0 rgba(0, 0, 0, 0.33), 0px 7px 5px 3px rgba(0, 0, 0, 0.11);
}
.nav-area ul li{
    display: inline-block;
    border-left: 1px dotted #777777;
    margin: 0px 1px;
    position: relative
}
.nav-area ul li ul {
    position: absolute;
    top: 100%;
    left: 0;
    width: 180px;
    background: #111;
    z-index: 99;
    box-shadow: 0 0 0;
    visibility: hidden;
    opacity: 0
}
.nav-area ul li:hover ul{
    visibility: visible;
    opacity: 1
}
.nav-area ul li ul li{
    display: block;
    border-left: 0px dotted #777777
}
.nav-area ul li ul li a{
    display: block;
    background: transparent;
    width: auto;
    text-transform: capitalize;
    color: #fff;
    padding: 7px 10px
}
.nav-area ul li a{
    color: #098145;
    background: #fff;
    padding: 10px 24px;
    display: block;
    font-weight: bold;
    width: 108%;
    
}
.nav-area ul li a:hover{ 
    background: #098145;
    color: #fff;
    transition: 0.7s;
}

