                    <i class="fa fa-bars"></i>
                </button>
                @Html.GetGadgetItem("SiteLogo")
            </div>
            <div class="jobguru-responsive-menu "></div>
            @Html.Partial("~/Views/Shared/frontend/" + this.Session["ThemeName"] + "/Menu/_Menu.cshtml")

