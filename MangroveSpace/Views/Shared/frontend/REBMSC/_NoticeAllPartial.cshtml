@model IEnumerable<dynamic>

@if (Model != null)
{


    
    <div class="page-content-right">
        <div class="teacher-list-table">
            <div class="theme-table">
                <table class="table table-striped table-responsive table-bordered">
                    <thead>
                        <tr>
                            <th>Publishing Date</th>
                            <th>Headline</th>
                            <th>See Details</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                        <tr>
                            @try
                            {
                                <td style="padding:10px">@item.NoticeDate.ToString("dd/MM/yyyy")</td>
                            }
                            catch
                            {

                            }
                            <td>@item.NoticeName</td>
                            <td><a href="/Notice/NoticeDetails/@item.NoticeID">Read more</a></td>
                        </tr>
                        }
                    </tbody>

                </table>
            </div>
        </div>
    </div>
}









